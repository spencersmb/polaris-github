{% block scripts %}
 <script>

	(function(){

		//change height of content to set offset paragraph correctly
		var belowContent = $('.below-content');
		var contentHeight = belowContent.height();
		var offset = parseInt(belowContent.css('top'));
		
		//Use addition here because the offset is negative value
		var overlapHeight = contentHeight + offset;

		//Set overlap height
		$('.overlap-bottom-wrapper').height(overlapHeight);

	})();
            
</script>
